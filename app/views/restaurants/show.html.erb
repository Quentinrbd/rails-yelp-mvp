<h2><%= @restaurant.name %></h2>
<p><%= @restaurant.address %></p>

<h3>reviews</h3>

<% @restaurant.reviews.each do |review| %>
  <p> <%= review.content %></p>
  <p> <%= review.rating %></p>
<% end %>

<%= link_to "Back to list", restaurants_path %>
<%= link_to "Add review", new_restaurant_review_path(@restaurant) %>
